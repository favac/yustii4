    "@nestjs/typeorm": "^8.0.2",
    "@types/jest": "^27.0.1",
    "amazon-cognito-identity-js": "^5.1.0",
    "await-to-js": "3.0.0",
    "dotenv": "10.0.0",
    "express-jwt": "^6.1.0",
    "helmet": "4.6.0",
